{
  "schemaVersion": 1,
  "id": "fabadditions",
  "version": "${version}",

  "name": "Fab's Additions",
  "description": "Fab's Additions is a Vanilla+ Mod for Fabric. Adding new useful items and decorations.",
  "authors": [
    "Brzzzn",
    "Tidari"
  ],
  "contact": {
    "homepage": "https://twitter.com/Brzzzn",
    "sources": "https://github.com/Brzzzn/FabAdditions"
  },

  "license": "MIT",
  "icon": "assets/fabadditions/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "brzzzn.fabadditions.FabAdditions"
    ],
    "client": [
      "brzzzn.fabadditions.FabAdditionsClient"
    ]
  },
  "mixins": [
    "fabadditions.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.12",
    "minecraft": "~1.19.2",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.9.1+kotlin.1.8.10"
  }
}